<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="upeu.agenda.exa.agendaexa">

    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_contact_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.PrincipalView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.addContactView"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".View.deleteContact"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".View.updateContact"
            android:theme="@style/AppTheme.CustomTheme"/>
    </application>

</manifest>